DEBUG=True

# Bot settings
## Token (required). Get it from @BotFather
BOT_TOKEN=__TOKEN_BOT_HERE__
## FSM Storage for game data. Values allowed: memory, redis
BOT_FSM_STORAGE=memory
## Token for open weather site
OPEN_WEATHER_TOKEN=__TOKEN_BOT_HERE__

# Storages
## Redis connection string. Required if BOT_FSM_STORAGE=redis
REDIS_DSN=redis://redis:6379/5
## PostgreSQL connection string. Always required 127.0.0.1', 5432, "postgres
POSTGRES_DSN=postgresql+asyncpg://postgres:postgres@postgresql:5432/postgres
